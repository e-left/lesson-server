generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model ancient_book_exercises {
  id            Int      @id @default(autoincrement())
  stamp_updated DateTime @default(now()) @db.Timestamp(0)
  chapter       String   @db.VarChar(25)
  ex_number     Int
  solution_path String   @db.VarChar(128)
}

model ancient_old_ex {
  id             Int      @id @default(autoincrement())
  stamp_updated  DateTime @default(now()) @db.Timestamp(0)
  ex_year        Int      @db.Year
  ex_path        String   @db.VarChar(128)
  solutions_path String   @db.VarChar(128)
}

model ancient_translations {
  id                 Int      @id @default(autoincrement())
  stamp_updated      DateTime @default(now()) @db.Timestamp(0)
  chapter            String   @db.VarChar(25)
  order_number       Int
  original_text      String   @db.VarChar(250)
  translated_text    String   @db.VarChar(250)
  syntactic_analysis String   @db.VarChar(300)
}

